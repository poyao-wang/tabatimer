{
  "expo": {
    "name": "TabaTimer",
    "slug": "TabaTimer",
    "version": "1.1.1",
    "orientation": "default",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f2f2f2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poyaowang.tabatimer",
      "buildNumber": "1.1.1"
    },
    "android": {
      "package": "com.poyaowang.tabatimer",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#f2f2f2"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
